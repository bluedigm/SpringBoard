<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//dtd mapper 3.0//en"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.user">
	<insert id="insert">
		insert into user(username,nickname,password)
		values(#{email},#{nick},#{password})
	</insert>
	<select id="select">
		select * from example
		where (username =='%#{}%')
	</select>
	<select id="search">
		select * from user where (key
		like '%#{}%' and str
		like '%#{}%')
	</select>
</mapper>