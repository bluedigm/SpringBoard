<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//dtd mapper 3.0//en"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bluedigm.springboard.entity.UserDAO">
	<insert id="insertUser">
		insert into user(email,nick,pw) values(#{email},#{nick},#{password})
	</insert>
</mapper>